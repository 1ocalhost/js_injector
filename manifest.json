{
  "name": "JS URL Injector",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Inject JS code from custom URL into every web frame",
  "icons": {
    "128": "rat.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>",
    "https://www.google.com/_/chrome/newtab*",
    "tabs",
    "webNavigation",
    "storage"
  ]
}